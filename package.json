    "Johan Öbrink <johan.obrink@iteam.se>",
    "Alexander Czigler <alexander@iteam.se>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/JobtechSwe/mydata/issues"
  },
  "homepage": "https://github.com/JobtechSwe/mydata#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "change-case": "^3.1.0",
    "changecase-objects": "^1.1.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "dropbox-fs": "^0.3.0",
    "elastic-apm-node": "^2.9.0",
    "express": "~4.16.0",
    "http-errors": "^1.6.3",
    "ioredis": "^3.2.2",
    "joi": "^13.7.0",
    "jsonwebtoken": "^8.3.0",
    "jwks-manager": "^0.9.1",
    "jwks-provider": "^0.1.0",
    "memory-fs": "^0.4.1",
    "merge": "^1.2.1",
    "moment": "^2.24.0",
    "morgan": "~1.9.0",
    "node-pg-migrate": "^3.15.0",
    "pg": "^7.8.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/ioredis": "^4.0.6",
    "@types/jest": "^23.3.13",
