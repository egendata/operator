{
  "name": "mydata-operator",
  "version": "0.27.0",
  "description": "Backend for managing consents and data flow",
  "scripts": {
    "start": "node ./lib/server.js",
    "start:watch": "nodemon ./lib/server.js",
    "dev": "npm run migrate up && NODE_ENV=development nodemon --inspect ./lib/server.js",
    "test": "jest -c jest.config.js --color",
    "test:watch": "jest -c jest.config.js --watchAll",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest -c jest.config.js --runInBand",
    "integration": "jest -c jest.integration.config.js",
    "integration:watch": "jest -c jest.integration.config.js --watchAll",
    "lint": "eslint '**/*.js'",
    "migrate": "node-pg-migrate",
    "testAndLint": "npm test && npm run lint"
  },
  "contributors": [
    "Adam Näslund <adam.naslund@iteam.se>",
    "Einar Persson <einar.persson@iteam.se>",
    "Johan Öbrink <johan.obrink@iteam.se>",
    "Alexander Czigler <alexander@iteam.se>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/egendata/operator/issues"
  },
  "homepage": "https://github.com/egendata/operator#readme",
  "dependencies": {
    "@egendata/messaging": "0.4.2",
    "@panva/jose": "1.0.2",
    "axios": "^0.18.1",
    "body-parser": "^1.19.0",
    "change-case": "3.1.0",
    "changecase-objects": "1.1.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "dotenv": "8.0.0",
    "dropbox-fs": "0.3.1",
    "elastic-apm-node": "2.11.1",
    "express": "4.16.4",
    "http-errors": "1.7.2",
    "ioredis": "3.2.2",
    "memory-fs": "0.4.1",
    "merge": "1.2.1",
    "moment": "2.24.0",
    "morgan": "1.9.1",
    "node-pg-migrate": "3.20.0",
    "pg": "7.11.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@types/axios": "0.14.0",
    "@types/ioredis": "4.0.10",
    "@types/jest": "24.0.12",
    "@types/supertest": "2.0.7",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jest": "22.5.1",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "get-value": "3.0.1",
    "jest": "24.8.0",
    "jest-spec-reporter": "1.0.5",
    "nodemon": "1.19.0",
    "object-path": "0.11.4",
    "semantic-release": "^15.13.24",
    "semantic-release-docker": "^2.2.0",
    "supertest": "4.0.2"
  }
}
